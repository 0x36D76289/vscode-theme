    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#666666"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#E0E0E0"
            }
        },
        {
            "name": "Variable Properties",
            "scope": [
                "variable.object.property",
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#5AC8FF"
            }
        },
        {
            "name": "Language Keywords",
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator",
                "keyword.other",
                "storage.type",
                "storage.modifier",
                "keyword.control.import",
                "keyword.control.export",
                "keyword.control.from",
                "keyword.control.as",
                "keyword.control.default",
                "keyword.control.new",
                "keyword.control.throw",
                "keyword.control.return",
                "keyword.control.flow",
                "keyword.control.conditional",
                "keyword.control.loop",
                "keyword.control.exception",
                "keyword.operator.logical",
                "keyword.operator.comparison",
                "keyword.operator.arithmetic",
                "keyword.operator.assignment",
                "keyword.operator.word",
                "keyword.other.unit",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#2883FF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Types",
            "scope": [
                "entity.name.type",
                "support.type",
                "support.class",
                "support.type.primitive",
                "support.type.builtin",
                "support.type.property-name",
                "entity.name.class",
                "entity.other.inherited-class",
                "storage.type.class",
                "storage.type.function",
                "storage.type.interface",
                "storage.type.enum",
                "storage.type.struct",
                "storage.type.union",
                "storage.type.generic",
                "storage.type.annotation",
                "storage.type.primitive",
                "support.type.sys-types"
            ],
            "settings": {
                "foreground": "#5AC8FF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "support.function.builtin",
                "support.function.magic",
                "support.function.construct",
                "support.function.any-method",
                "meta.function-call",
                "variable.function"
            ],
            "settings": {
                "foreground": "#4362ff",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Operators and Punctuation",
            "scope": [
                "punctuation",
                "meta.tag",
                "punctuation.definition.tag",
                "punctuation.separator.inheritance.php",
                "punctuation.definition.parameters",
                "punctuation.definition.array",
                "punctuation.separator.delimiter",
                "punctuation.accessor"
            ],
            "settings": {
                "foreground": "#CCCCCC"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.other",
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#3D2AFF"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "string.interpolated",
                "string.regexp",
                "constant.other.symbol"
            ],
            "settings": {
                "foreground": "#7129FF"
            }
        },
        {
            "name": "Tags and Attributes",
            "scope": [
                "entity.name.tag",
                "entity.other.attribute-name",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#BA5AFF"
            }
        },
        {
            "name": "Classes and Entities",
            "scope": [
                "entity.name",
                "entity.name.class",
                "entity.name.namespace",
                "entity.name.module",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#685AFF"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#A52AFF",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Language Variables",
            "scope": [
                "variable.language",
                "variable.language.this",
                "variable.language.self",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#FF6B9D"
            }
        },
        {
            "name": "Markdown Headers",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#2883FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#F1F1F1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#7129FF",
                "fontStyle": "italic"
