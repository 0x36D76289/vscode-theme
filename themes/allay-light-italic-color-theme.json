    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#808080"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#2B2B2B"
            }
        },
        {
            "name": "Variable Properties",
            "scope": [
                "variable.object.property",
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#1565C0"
            }
        },
        {
            "name": "Language Keywords",
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator",
                "keyword.other",
                "storage.type",
                "storage.modifier",
                "keyword.control.import",
                "keyword.control.export",
                "keyword.control.from",
                "keyword.control.as",
                "keyword.control.default",
                "keyword.control.new",
                "keyword.control.throw",
                "keyword.control.return",
                "keyword.control.flow",
                "keyword.control.conditional",
                "keyword.control.loop",
                "keyword.control.exception",
                "keyword.operator.logical",
                "keyword.operator.comparison",
                "keyword.operator.arithmetic",
                "keyword.operator.assignment",
                "keyword.operator.word",
                "keyword.other.unit",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#0059B3",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Types",
            "scope": [
                "entity.name.type",
                "support.type",
                "support.class",
                "support.type.primitive",
                "support.type.builtin",
                "support.type.property-name",
                "entity.name.class",
                "entity.other.inherited-class",
                "storage.type.class",
                "storage.type.function",
                "storage.type.interface",
                "storage.type.enum",
                "storage.type.struct",
                "storage.type.union",
                "storage.type.generic",
                "storage.type.annotation",
                "storage.type.primitive",
                "support.type.sys-types"
            ],
            "settings": {
                "foreground": "#00796B",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function",
                "support.function.builtin",
                "support.function.magic",
                "support.function.construct",
                "support.function.any-method",
                "meta.function-call",
                "variable.function"
            ],
            "settings": {
                "foreground": "#4362ff",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Operators and Punctuation",
            "scope": [
                "punctuation",
                "meta.tag",
                "punctuation.definition.tag",
                "punctuation.separator.inheritance.php",
                "punctuation.definition.parameters",
                "punctuation.definition.array",
                "punctuation.separator.delimiter",
                "punctuation.accessor"
            ],
            "settings": {
                "foreground": "#37474F"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.other",
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#3824b3"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "string.interpolated",
                "string.regexp",
                "constant.other.symbol"
            ],
            "settings": {
                "foreground": "#6A0DAD"
            }
        },
        {
            "name": "Tags and Attributes",
            "scope": [
                "entity.name.tag",
                "entity.other.attribute-name",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#9800D6"
            }
        },
        {
            "name": "Classes and Entities",
            "scope": [
                "entity.name",
                "entity.name.class",
                "entity.name.namespace",
                "entity.name.module",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#3824b3"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#9800D6",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Language Variables",
            "scope": [
                "variable.language",
                "variable.language.this",
                "variable.language.self",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#C2185B"
            }
        },
        {
            "name": "Markdown Headers",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#0059B3",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#37474F",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#6A0DAD",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown Code",
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code"
            ],
            "settings": {
                "foreground": "#3824b3"
            }
        },
        {
            "name": "Python Decorators",
            "scope": [
                "entity.name.function.decorator.python",
                "meta.function.decorator.python"
            ],
            "settings": {
                "foreground": "#E91E63"
            }
        },
        {
            "name": "Python Self Parameter",
            "scope": [
                "variable.parameter.function.language.special.self.python"
            ],
            "settings": {
                "foreground": "#C2185B"
            }
        },
        {
            "name": "Java Annotations",
            "scope": [
                "storage.type.annotation.java"
            ],
            "settings": {
                "foreground": "#E91E63"
            }
        },
        {
            "name": "C/C++ Preprocessor",
            "scope": [
                "keyword.control.directive",
                "meta.preprocessor"
            ],
            "settings": {
                "foreground": "#7B1FA2"
            }
        },
        {
            "name": "Rust Lifetime",
            "scope": [
                "storage.modifier.lifetime.rust",
                "entity.name.lifetime.rust"
            ],
            "settings": {
                "foreground": "#FF6B35"
            }
        },
        {
            "name": "Go Package",
            "scope": [
                "entity.name.package.go"
            ],
            "settings": {
                "foreground": "#00796B"
            }
        },
        {
            "name": "PHP Variables",
            "scope": [
                "variable.other.php",
