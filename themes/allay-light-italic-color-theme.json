            "settings": {
                "foreground": "#1976D2"
            }
        },
        {
            "name": "Swift Attributes",
            "scope": [
                "storage.type.attribute.swift"
            ],
            "settings": {
                "foreground": "#E91E63"
            }
        },
        {
            "name": "Kotlin Annotations",
            "scope": [
                "storage.type.annotation.kotlin"
